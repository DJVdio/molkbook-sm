# SecondMe OAuth Configuration
# Copy this file to .env and fill in your actual values
SECONDME_CLIENT_ID=your-client-id-here
SECONDME_CLIENT_SECRET=your-client-secret-here
SECONDME_REDIRECT_URI=http://localhost:3000/auth/callback

# JWT Secret (generate a random 256-bit key)
JWT_SECRET=your-256-bit-secret-key-here

# Database (for production)
DB_HOST=localhost
DB_PORT=3306
DB_NAME=molkbook
DB_USERNAME=root
DB_PASSWORD=your-password
